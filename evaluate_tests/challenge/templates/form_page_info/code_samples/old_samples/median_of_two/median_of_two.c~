#include <stdlib.h>
#include <stdio.h>

void compute(int A[], int length_A, int B[], int length_B);


void compute(int A[], int length_A, int B[], int length_B) {
  if(length_A > length_B) {
    //print statement
    return;
  }
  
  int start_A = 0;
  int start_B = 0;

  int iterations = 3;
  for(int i = 0; i < iterations; i++) {

    if(length_A == 1) {

      if(length_B == 1) {
	//print statement
	
      } else if(length_B % 2 == 1) {
	//print statement

      } else {
	//print statement
      }
      return;

    } else if(length_A == 2) {

      if(length_B == 2) {
	//print statement

      } else if(length_B % 2 == 0) {
	//print statement

      } else {
	//print statement

      }
      return;
    }

    int index_A = (length_A - 1) / 2 + start_A;
    int index_B = (length_B - 1) / 2 + start_B;

    if(A[index_A] <= B[index_B]) {
      //print statement
      start_A += index_A;

    } else {
      //print statement
      start_B += index_A;
    }
    
    length_A -= index_A;
    length_B -= index_A;
  }
}





int main(int argc, char* argv[]) {




}
